{
	"name": "trcprnt",
	"version": "0.0.0",
	"description": "Blazing fast w3c traceparent ids",
	"keywords": [
		"tracecontext",
		"traceparent",
		"distributed",
		"tracing",
		"w3c"
	],
	"repository": "maraisr/t",
	"license": "MIT",
	"type": "module",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"sideEffects": false,
	"imports": {
		"#hex": {
			"node": "./imports/hex/node.js",
			"default": "./imports/hex/default.js"
		}
	},
	"files": [
		"imports/**/*"
	],
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./index.js",
	"module": "./index.mjs",
	"types": "index.d.ts",
	"scripts": {
		"bench": "node -r tsm bench/index.ts",
		"format": "prettier  --write --list-different \"{*,src/**/*,test/*,bench/*,.github/**/*}.+(ts|json|yml|md)\"",
		"test": "uvu test \".spec.ts$\" -r tsm",
		"typecheck": "tsc --noEmit  --skipLibCheck"
	},
	"devDependencies": {
		"@marais/tsconfig": "0.0.1",
		"@types/node": "16.11.7",
		"prettier": "2.4.1",
		"tsm": "2.1.4",
		"typescript": "4.5.2",
		"worktop": "0.8.0-next.8",
		"uvu": "0.5.2"
	},
	"volta": {
		"node": "16.12.0"
	},
	"peerDependencies": {
		"worktop": "0.8.0-next.8"
	},
	"peerDependenciesMeta": {
		"worktop": {
			"optional": true
		}
	}
}
