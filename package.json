{
	"name": "tctx",
	"version": "0.0.5",
	"description": "W3C Trace Contexts made simple",
	"keywords": [
		"tracecontext",
		"traceparent",
		"distributed",
		"tracing",
		"w3c"
	],
	"repository": "maraisr/tctx",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./index.js",
	"module": "./index.mjs",
	"types": "index.d.ts",
	"files": [
		"index.mjs",
		"index.js",
		"index.d.ts"
	],
	"scripts": {
		"bench": "node -r tsm bench/index.ts",
		"build": "rollup -c",
		"format": "prettier  --write --list-different \"{*,src/**/*,imports/*/*,test/*,bench/*,.github/**/*}.+(ts|js|json|yml|md)\"",
		"test": "uvu test \".spec.ts$\" -r tsm",
		"typecheck": "tsc --noEmit  --skipLibCheck"
	},
	"dependencies": {
		"@lukeed/csprng": "^1.0.0"
	},
	"devDependencies": {
		"@marais/tsconfig": "0.0.1",
		"@types/node": "16.11.9",
		"prettier": "2.4.1",
		"rollup": "2.60.0",
		"rollup-plugin-dts": "4.0.1",
		"tsm": "2.1.4",
		"typescript": "4.5.2",
		"uvu": "0.5.2"
	},
	"engines": {
		"node": ">=14"
	},
	"volta": {
		"node": "16.12.0"
	}
}
